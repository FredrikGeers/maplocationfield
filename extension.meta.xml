<extension id="maplocationfield">
	
	<name>Map Location field</name>

	<description lang="en">Plot a location on a Google Map. Supports a radius data source filter for "store locator" type searching.</description>

	<url type="repo">https://github.com/nickdunn/maplocationfield</url>
	
	<types>
		<type>Interface</type>
		<type>Field</type>
	</types>
	
